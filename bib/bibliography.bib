@inproceedings{Cattuto2008,
abstract = {Navigational features have been largely recognized as fundamental$\backslash$nfor graph database query languages. This fact has motivated several authors to$\backslash$npropose RDF query languages with navigational capabilities. In particular, we$\backslash$nhave argued in a previous paper that nested regular expressions are appropriate$\backslash$nto navigate RDF data, and we have proposed the nSPARQL query language$\backslash$nfor RDF, that uses nested regular expressions as building blocks. In this paper,$\backslash$nwe study some of the fundamental properties of nSPARQL concerning expressiveness$\backslash$nand complexity of evaluation. Regarding expressiveness, we show that$\backslash$nnSPARQL is expressive enough to answer queries considering the semantics of$\backslash$nthe RDFS vocabulary by directly traversing the input graph. We also show that$\backslash$nnesting is necessary to obtain this last result, and we study the expressiveness of$\backslash$nthe combination of nested regular expressions and SPARQL operators. Regarding$\backslash$ncomplexity of evaluation, we prove that the evaluation of a nested regular$\backslash$nexpression E over an RDF graph G can be computed in time O(|G| {\textperiodcentered} |E|).},
author = {Cattuto, Ciro and Benz, Dominik and Hotho, Andreas and Stumme, Gerd},
booktitle = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
doi = {10.1007/978-3-540-88564-1-39},
